{
  "name": "tienda-web-lotoai",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:backend": "cd src/backend && npm run build",
    "build:all": "npm run build:backend && npm run build:prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr": "node dist/tienda-web-loto-ai/server/main.server.mjs",
    "serve:ssr:dev": "node dist/tienda-web-loto-ai/server/main.server.mjs",
    "build:ssr": "ng build && ng run tienda-web-loto-ai:server",
    "serve:ssr:tienda-web-loto-ai": "node dist/tienda-web-loto-ai/server/main.server.mjs",
    "prerender": "ng run tienda-web-loto-ai:prerender",
    "build:scraper": "tsc -p tsconfig.scraper.json",
    "scraper:results": "cd scraper && npx ts-node scraper-extended.ts",
    "scraper:botes": "cd scraper && npx ts-node scraper.ts",
    "scraper:all": "npm run scraper:botes && npm run scraper:results"
  },
  "dependencies": {
    "@angular/animations": "^18.2.12",
    "@angular/common": "^18.2.12",
    "@angular/compiler": "^18.2.12",
    "@angular/core": "^18.2.12",
    "@angular/forms": "^18.2.12",
    "@angular/platform-browser": "^18.2.12",
    "@angular/platform-browser-dynamic": "^18.2.12",
    "@angular/platform-server": "^18.2.12",
    "@angular/router": "^18.2.12",
    "@angular/ssr": "^18.2.12",
    "@stripe/stripe-js": "^4.8.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.3",
    "canvas-confetti": "^1.9.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "ngx-bootstrap": "^18.1.3",
    "node-mailjet": "^6.0.5",
    "pg": "^8.12.0",
    "rxjs": "^7.8.1",
    "stripe": "^17.7.0",
    "three": "^0.177.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.12",
    "@types/bcrypt": "^5.0.2",
    "@types/canvas-confetti": "^1.9.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^18.19.64",
    "@types/node-fetch": "^2.6.12",
    "@types/pg": "^8.15.4",
    "@types/three": "^0.177.0",
    "dotenv": "^17.0.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.0"
  }
}
