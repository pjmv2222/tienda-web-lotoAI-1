{
  "name": "tienda-web-lotoai",
  "version": "1.0.0",
  "scripts": {
    "build": "ng build --configuration production",
    "build:ssr": "ng build && ng build --ssr && node fix-build.js",
    "build:server": "ng build --configuration production --ssr",
    "build:backend": "cd src/backend && npm install --production=false && npm run build",
    "serve:ssr": "node dist/tienda-web-loto-ai/server/main.server.mjs",
    "serve:express": "node express-server.js",
    "start": "ng serve",
    "start:prod": "npm run serve:ssr",
    "deploy:frontend": "bash scripts/deploy-frontend.sh"
  },
