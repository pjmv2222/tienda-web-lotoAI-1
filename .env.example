# Variables de entorno para Loto-IA
# Copia este archivo como .env y completa con tus valores reales

# Base de datos PostgreSQL
POSTGRES_PASSWORD=tu_password_aqui

# JWT para autenticación
JWT_SECRET=tu_jwt_secret_aqui

# Mailjet para envío de emails
MAILJET_API_KEY=tu_mailjet_api_key_aqui
MAILJET_API_SECRET=tu_mailjet_api_secret_aqui

# Stripe para pagos (si se usa)
STRIPE_SECRET_KEY=tu_stripe_secret_key_aqui
STRIPE_PUBLISHABLE_KEY=tu_stripe_publishable_key_aqui
